    "grantPolicies": {
      "xrn:firebolt:capability:data:app-usage": {
        "use": {
          "options": [
            {
              "steps": [
                {
                  "capability": "xrn:firebolt:capability:usergrant:acknowledgechallenge"
                }
              ]
            }
          ],
          "scope": "app",
          "lifespan": "seconds",
          "overridable": false,
          "lifespanTtl": 31536000,
          "persistence": "account",
          "evaluateAt": [
            "activeSession"
          ]
        }
      }
    },
    "dependencies": {
