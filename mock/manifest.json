    "grantPolicies": {
      "xrn:firebolt:capability:data:app-usage": {
        "use": {
          "options": [
            {
              "steps": [
                {
                  "capability": "xrn:firebolt:capability:usergrant:acknowledgechallenge"
                }
              ]
            }
          ],
          "scope": "app",
          "lifespan": "seconds",
          "overridable": false,
          "lifespanTtl": 31536000,
          "persistence": "account",
          "evaluateAt": [
            "activeSession"
          ]
        }
      }
    },
    "dependencies": {
      "xrn:firebolt:capability:discovery:watched": [
        "xrn:firebolt:capability:data:app-usage"
      ],
      "xrn:firebolt:capability:discovery:watch-next": [
        "xrn:firebolt:capability:data:app-usage"
      ]
    },
    "grantExclusionFilters": [
      {
        "catalog": "cstore"
      }
    ]
  },
  "lifecycle": {
    "appFinishedTimeoutMs": 500
  },
  "applications": {
    "defaults": {
      "xrn:firebolt:application-type:main": "com.bskyb.epgui",
      "xrn:firebolt:application-type:settings": "com.bskyb.epgui"
